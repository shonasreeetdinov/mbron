{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mbron": {
      "projectType": "application",
      "sourceRoot": "src",
      "root": "",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/web",
            "index": "src/index-web.html",
            "browser": "src/main-web.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/assets", "src/favicon.ico"],
            "styles": ["src/styles-web.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            },
          "mobile": {
          "outputPath": {
            "base": "dist/mobile",
            "browser": ""
          },
          "browser": "src/main-mobile.ts",
          "index": { "input": "src/index-mobile.html", "output": "index.html" },
          "styles": ["src/styles-mobile.scss"],
          "optimization": false,
          "sourceMap": true
          },
            "mobile-production": {
              "outputPath": "dist/mobile",
              "browser": "src/main-mobile.ts",
              "index": "src/index-mobile.html",
              "styles": ["src/styles-mobile.scss"],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "mbron:build:development"
            },
            "production": {
              "buildTarget": "mbron:build:production"
            },
            "mobile": {
              "buildTarget": "mbron:build:mobile"
            },
            "mobile-production": {
              "buildTarget": "mbron:build:mobile-production"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}