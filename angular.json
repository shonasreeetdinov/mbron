{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mbron-web": {
      "projectType": "application",
      "root": "apps/web",
      "sourceRoot": "apps/web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/web",
            "index": "apps/web/src/index.html",
            "browser": "apps/web/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/web/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/web/public"
              },
              "apps/web/src/favicon.ico",
              "apps/web/src/assets"
            ],
            "styles": [
              "apps/web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "libs/core/src/lib/config/environment.ts",
                  "with": "libs/core/src/lib/config/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "mbron-web:build:development",
              "port": 4200
            },
            "production": {
              "buildTarget": "mbron-web:build:production",
              "port": 4200
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "mbron-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/web/tsconfig.spec.json",
            "assets": [
              "apps/web/src/favicon.ico",
              "apps/web/src/assets"
            ],
            "styles": [
              "apps/web/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "mbron-mobile": {
      "projectType": "application",
      "root": "apps/mobile",
      "sourceRoot": "apps/mobile/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/mobile",
            "index": "apps/mobile/src/index.html",
            "browser": "apps/mobile/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/mobile/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/mobile/public"
              },
              "apps/mobile/src/favicon.ico",
              "apps/mobile/src/assets"
            ],
            "styles": [
              "apps/mobile/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "libs/core/src/lib/config/environment.ts",
                  "with": "libs/core/src/lib/config/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            },
            "mobile-production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": {
                  "inline": false
                }
              },
              "namedChunks": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "libs/core/src/lib/config/environment.ts",
                  "with": "libs/core/src/lib/config/environment.prod.ts"
                }
              ],
              "allowedCommonJsDependencies": [
                "@stencil/core"
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "mbron-mobile:build:development",
              "port": 4201,
              "proxyConfig": "apps/mobile/proxy.conf.json"
            },
            "production": {
              "buildTarget": "mbron-mobile:build:production",
              "port": 4201
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "mbron-mobile:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/mobile/tsconfig.spec.json",
            "assets": [
              "apps/mobile/src/favicon.ico",
              "apps/mobile/src/assets"
            ],
            "styles": [
              "apps/mobile/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}